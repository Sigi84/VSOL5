<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.canvas.Canvas?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane fx:id="ancRoot" prefHeight="600.0" prefWidth="800.0" stylesheets="@css/style.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="be.veterinarysolutions.vsol.gui.Viewer">
   <children>
      <VBox fx:id="vboxRight" alignment="TOP_CENTER" layoutX="712.0" layoutY="10.0" spacing="10.0" AnchorPane.bottomAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Button fx:id="btnOpen" mnemonicParsing="false" onAction="#btnOpenAction">
               <graphic>
                  <ImageView fitHeight="24.0" fitWidth="24.0" pickOnBounds="true">
                     <image>
                        <Image url="@../ico/folder-open-regular.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
            <Button fx:id="btnRotate" mnemonicParsing="false" onAction="#btnRotateAction">
               <graphic>
                  <ImageView fitHeight="24.0" fitWidth="24.0" pickOnBounds="true">
                     <image>
                        <Image url="@../ico/Ic_screen_rotation_48px.svg.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
      <Canvas fx:id="canvas" height="244.0" layoutX="302.0" layoutY="300.0" width="369.0" AnchorPane.leftAnchor="302.0" AnchorPane.topAnchor="300.0" />
   </children>
</AnchorPane>
